datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id          String   @id @default(cuid())
  nickName    String   @unique
  name        String
  password    String
  description String
  dateBirth   DateTime
  followers     User[]   @relation("follow")
  followersBy   User[]   @relation("follow")
}
